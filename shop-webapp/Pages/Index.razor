@page "/"
@inject shop.ApiClient client

<PageTitle>Index</PageTitle>

<p>Here's a some json</p>
<ul>@foreach (var item in items) {
    <li>@item</li>
}
</ul>


@code {
    public List<string> items { get; set; } = new List<string>();
    protected override void OnInitialized()
    {
        var res = client.AllAsync().GetAwaiter().GetResult();
        items = res.Select(i => i.Name).ToList();

        base.OnInitialized();
    }
}